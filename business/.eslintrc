{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "object-curly-newline": ["error", { "consistent": true }],
    "semi": [2, "never"],
    "semi-style": [2, "first"],
    "camelcase": 0,
    "indent": [2, "tab", { "SwitchCase": 1 }],
    "no-tabs": 0,
    "max-len": 0
  }
}
